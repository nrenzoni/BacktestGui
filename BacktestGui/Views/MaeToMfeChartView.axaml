             xmlns:viewModels="clr-namespace:BacktestGui.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="BacktestGui.Views.MaeToMfeChartView">

    <Design.DataContext>
        <viewModels:MaeToMfeChartViewModel />
    </Design.DataContext>

    <StackPanel>
        <avalonia:AvaPlot
